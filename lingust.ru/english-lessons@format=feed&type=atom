	<entry>
		<title>Урок 1. Английский алфавит. О курсе</title>
		<link rel="alternate" type="text/html" href="https://lingust.ru/english/english-lessons/lesson1"/>
		<published>2020-02-16T06:25:22+00:00</published>
		<updated>2020-02-16T06:25:22+00:00</updated>
